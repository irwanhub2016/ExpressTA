extends ../layout/base_monitoring 
block content         
        section.content-header
          h1
            | Data Pengisian Air
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-dashboard
                |  Home
            li
              a(href='#') Tables
            li.active Data tables
        // Main content
        section.content
          .row
            .col-xs-12
              .box
                .box-header
                  h3.box-title Data Table With Full Features
                // /.box-header
                .box-body
                  table#example1.table.table-bordered.table-striped
                    thead
                      tr
                        th ID
                        th Date
                        th Time
                        th Harga
                        th Berat
                    for row in data 
                        tr
                          td #{row.id_pengisian}
                          td #{row.date_pengisian}
                          td #{row.time}
                          td #{row.harga}
                          td #{row.berat}                          